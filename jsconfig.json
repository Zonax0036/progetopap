{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@/*": [
                "./*"
            ]
        },
        "checkJs": false,
        "allowJs": true
    },
    "exclude": [
        "node_modules"
    ]
}